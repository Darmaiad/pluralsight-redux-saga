{
  "name": "pluralsight-redux-saga",
  "version": "1.0.0",
  "description": "Re-implementation of the project developed in the Pluralsight course: \"Redux Saga\" by Daniel Stern.",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development babel-node server/devServer.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch server/**/* --exec babel-node --inspect server/devServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prod:html": "babel-node server/buildHTML.js",
    "preprod": "npm-run-all clean-dist prod:html",
    "prod": "cross-env NODE_ENV=production babel-node server/build.js",
    "postprod": "cross-env NODE_ENV=production babel-node server/prodServer.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chance": "^1.0.16",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "socket.io": "^2.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jest": "^20.0.4",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-tool": "^1.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.24.2"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-regenerator",
      "transform-object-rest-spread"
    ]
  },
  "author": "Darmaiad",
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "nodejs",
    "redux-saga",
    "immutablejs",
    "express",
    "reselect",
    "es6",
    "reactjs",
    "javascript",
    "jest",
    "babel",
    "webpack",
    "continuous-integration",
    "continuous-deployment",
    "travis-ci",
    "heroku",
    "appveyor"
  ],
  "main": "./src/index.js",
  "license": "ISC",
  "homepage": "https://pluralsight-redux-saga.herokuapp.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Darmaiad/pluralsight-redux-saga.git"
  }
}
